{
    "name": "konfig/{{packageName}}",
    "description": "{{{appDescription}}}",
    "keywords": [
        "openapi",
        "php",
        "sdk",
        "rest",
        "konfig",
        "api"
    ],
    "license": "unlicense",
    "authors": [
        {
            "name": "{{{packageAuthor}}}",
            "homepage": "{{{packageAuthorHomepage}}}"
        }
    ],
    "require": {
        "php": "{{#supportPhp7}}>=7.0{{/supportPhp7}}{{^supportPhp7}}^8.0{{/supportPhp7}}",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "{{#supportPhp7}}^3.3{{/supportPhp7}}{{^supportPhp7}}^3.5{{/supportPhp7}}"
    },
    "autoload": {
        "psr-4": { "{{escapedInvokerPackage}}\\" : "{{srcBasePath}}/" }
    },
    "autoload-dev": {
        "psr-4": { "{{escapedInvokerPackage}}\\Test\\" : "{{testBasePath}}/" }
    }
}
