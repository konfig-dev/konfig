[tool.poetry]
name = "{{projectName}}"
version = "{{packageVersion}}"
description = "Client for {{appName}}"
authors = ["{{#if infoName}}{{infoName}}{{else}}Konfig{{/if}} <{{#if infoEmail}}{{infoEmail}}{{else}}engineering@konfigthis.com{{/if}}>"]
license = "MIT"
readme = "README.md"
packages = [{include = "{{packageName}}"}]

[tool.poetry.dependencies]
{{{poetryDependencies}}}

[tool.poetry.group.dev.dependencies]
setuptools = "^65.5.1"
pytest = "^7.3.1"
pytest-cov = "2.8.1"
pytest-randomly = "1.2.3"
build = "^0.10.0"
twine = "^4.0.2"
pytest-asyncio = "^0.21.0"
black = "^23.3.0"

[build-system]
requires = ["poetry-core"]
build-backend = "poetry.core.masonry.api"
