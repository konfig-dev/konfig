{
  "name": "konfig-docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start --no-open --poll 1000",
    "build": "docusaurus build && yarn check-links",
    "check-links": "yarn hyperlink build/ --sources src/ --check-anchors",
    "lychee": "lychee build --exclude ycombinator --exclude googletagmanager --exclude npmjs",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "gen-all": "docusaurus gen-api-docs all",
    "clean-all": "docusaurus clean-api-docs all",
    "re-gen": "yarn clean-all && yarn gen-all"
  },
  "dependencies": {
    "@code-hike/mdx": "^0.9.0",
    "@docusaurus/core": "^3.1.1",
    "@docusaurus/plugin-google-gtag": "^3.1.1",
    "@docusaurus/preset-classic": "^3.1.1",
    "@docusaurus/theme-mermaid": "^3.1.1",
    "@easyops-cn/docusaurus-search-local": "^0.40.1",
    "@fortawesome/fontawesome-free": "^6.4.0",
    "@mdx-js/react": "^3.0.1",
    "@radix-ui/react-collapsible": "^1.0.3",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-slot": "^1.0.2",
    "@stackblitz/sdk": "^1.9.0",
    "@tabler/icons-react": "^2.45.0",
    "@types/react-slick": "^0.23.10",
    "autoprefixer": "^10.4.14",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "cmdk": "^1.0.0",
    "docusaurus-json-schema-plugin": "^1.6.0",
    "express": "^4.18.2",
    "konfig-code-hike-mdx": "0.9.0",
    "konfig-lib": "^1.35.0",
    "lucide-react": "^0.358.0",
    "marked": "^11.1.1",
    "mermaid": "^10.9.0",
    "mobx": "^6.12.0",
    "mobx-persist-store": "^1.1.4",
    "mobx-react-lite": "^4.0.6",
    "moment": "^2.30.1",
    "party-js": "^2.2.0",
    "postcss": "^8.4.25",
    "prism-react-renderer": "^2.3.1",
    "react": "^18.2.0",
    "react-calendly": "^4.1.1",
    "react-device-detect": "^2.2.3",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0",
    "react-innertext": "^1.1.5",
    "react-player": "^2.12.0",
    "react-tooltip": "^5.15.0",
    "remark-footnotes": "^4.0.1",
    "remark-gfm": "^4.0.0",
    "swiper": "^10.1.0",
    "tailwind-merge": "^2.2.1",
    "tailwindcss": "^3.3.2",
    "tailwindcss-animate": "^1.0.7",
    "url": "^0.11.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^3.1.1",
    "@docusaurus/tsconfig": "^3.1.1",
    "@docusaurus/types": "^3.1.1",
    "@types/react": "^18.2.66",
    "@types/uuid": "^9.0.2",
    "@untitaker/hyperlink": "^0.1.32",
    "typescript": "^5.4.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "postman-collection": "3.6.6"
  },
  "engines": {
    "node": ">=18.0"
  }
